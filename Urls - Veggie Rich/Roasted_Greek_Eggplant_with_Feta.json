            "metric_amount": "44 ml"
        },
        {
            "name": "chopped fresh mint OR 1 teaspoon chopped fresh oregano",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "feta cheese (finely crumbled)",
            "us_amount": "3 tbsp",
            "metric_amount": "44 ml"
        }
