        {
            "name": "Cacao en polvo",
            "us_amount": "2 tbsp",
            "metric_amount": "30 ml"
        },
        {
            "name": "cobertura batida (sin grasa)",
            "us_amount": "1 cup",
            "metric_amount": "237 ml"
        }
