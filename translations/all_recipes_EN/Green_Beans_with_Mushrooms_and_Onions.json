        {
            "name": "mushrooms, such as button, cremini, or portobello (sliced)",
            "us_amount": "4 oz",
            "metric_amount": "113 ml"
        },
        {
            "name": "thinly sliced onion",
            "us_amount": "1/2 cup",
            "metric_amount": "118 ml"
        },
        {
