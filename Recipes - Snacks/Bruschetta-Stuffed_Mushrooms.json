    "servings": "7 Servings",
    "ingredients": [
        {
            "name": "grape tomatoes",
            "us_amount": "1 pints",
            "metric_amount": "480 ml"
        },
        {
            "name": "olive oil",
            "us_amount": "2 tsp",
            "metric_amount": "10 g"
        },
        {
            "name": "garlic",
            "us_amount": "2 diente",
            "metric_amount": "2 diente"
        },
        {
            "name": "dried basil",
            "us_amount": "2 tsp",
            "metric_amount": "10 g"
        },
        {
            "name": "2% mozzarella cheese",
            "us_amount": "1/3 cup",
            "metric_amount": "78 ml"
        },
        {
            "name": "freshly grated parmesan cheese",
            "us_amount": "2 tbsp",
            "metric_amount": "30 ml"
        },
        {
            "name": "large \"stuffer\" mushrooms",
            "us_amount": "14 oz (about 14 mushrooms)",
            "metric_amount": "14 oz (about 14 mushrooms)"
        },
        {
            "name": "balsamic vinegar",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        }
    ],
    "instructions": [
        "Preheat oven to 350 degrees F. Cut tomatoes into quarters and set aside.",
        "In a skillet, heat olive oil and add garlic. Saute garlic for about 1 minute. Add tomatoes and continue to saute for about 4 minutes. Remove from heat and stir in basil and both cheeses.",
        "Remove stems from mushrooms and fill with tomato mixture. Bake in oven for 15 minutes.",
        "Let cool slightly, then drizzle with balsamic vinegar. Serve warm."
    ],
