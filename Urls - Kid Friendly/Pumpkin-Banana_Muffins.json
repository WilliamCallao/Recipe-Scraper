    "ingredients": [
        {
            "name": "white whole wheat flour",
            "us_amount": "1 1/2 cup",
            "metric_amount": "355 ml"
        },
        {
            "name": "brown sugar",
            "us_amount": "1/3 cup",
            "metric_amount": "78 ml"
        },
        {
            "name": "baking soda",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        },
        {
            "name": "salt",
            "us_amount": "1/2 tsp",
            "metric_amount": "2 g"
        },
        {
            "name": "pumpkin pie spice",
            "us_amount": "2 tsp",
            "metric_amount": "10 g"
        },
        {
            "name": "eggs",
            "us_amount": "2",
            "metric_amount": "2"
        },
        {
            "name": "pumpkin puree",
            "us_amount": "1 cup",
            "metric_amount": "237 ml"
        },
        {
            "name": "canola oil",
            "us_amount": "3 tbsp",
            "metric_amount": "44 ml"
        },
        {
            "name": "banana(s) (mashed)",
            "us_amount": "1 small",
            "metric_amount": "1 small"
        },
        {
            "name": "vanilla extract",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        },
        {
            "name": "walnuts (finely chopped)",
            "us_amount": "1/4 cup",
            "metric_amount": "59 ml"
        }
    ],
    "instructions": [
        "Preheat the oven to 350 degrees F and line a muffin tin with liners.",
        "Mix flour, sugar, baking soda, salt, pumpkin pie spice in a large bowl.",
        "In a small bowl, mix together the eggs, pumpkin puree, oil, banana, and vanilla.",
        "Pour the wet ingredients into the dry ingredients and stir to combine. Gently fold in the walnuts.",
        "Scoop the batter into muffin tins and bake for 18-20 minutes, until a toothpick comes out clean."
    ],
