            "name": "sunflower or avocado oil (divided)",
            "us_amount": "3 tablespoons + 1/2 teaspoon",
            "metric_amount": "3 tablespoons + 1/2 teaspoon"
        },
        {
            "name": "unsalted butter (melted)",
            "us_amount": "2 tbsp",
            "metric_amount": "30 ml"
        },
        {
            "name": "pitted dates (about 1 cup)",
            "us_amount": "5 oz",
            "metric_amount": "142 ml"
        },
        {
            "name": "no-added-sugar apple butter (spread)",
            "us_amount": "2/3 cup",
            "metric_amount": "156 ml"
        },
        {
            "name": "eggs",
            "us_amount": "1 large",
            "metric_amount": "1 large"
        },
        {
            "name": "vanilla extract",
            "us_amount": "1 1/2 tsp",
            "metric_amount": "7 g"
        },
        {
            "name": "almond extract",
            "us_amount": "1/4 tsp",
            "metric_amount": "1 g"
        },
        {
            "name": "no-added-salt, no-added-sugar peanut butter",
            "us_amount": "1/2 cup",
            "metric_amount": "118 ml"
        },
        {
            "name": "sea salt",
            "us_amount": "3/4 tsp",
            "metric_amount": "4 g"
        },
        {
