            "metric_amount": "1 slice"
        },
        {
            "name": "egg whites",
            "us_amount": "2",
            "metric_amount": "2"
        },
        {
            "name": "hot pepper sauce",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "green onion (scallion) (minced)",
            "us_amount": "2",
            "metric_amount": "2"
        },
        {
            "name": "salt",
            "us_amount": "1/4 tsp",
            "metric_amount": "1 g"
        },
        {
            "name": "black pepper",
            "us_amount": "1/4 tsp",
            "metric_amount": "1 g"
        },
        {
            "name": "fancy white (or lump) crab meat) (drained)",
            "us_amount": "2 (6-oz) cans",
            "metric_amount": "2 (6-oz) cans"
        },
        {
            "name": "nonstick cooking spray",
            "us_amount": "1",
