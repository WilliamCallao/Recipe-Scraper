    "ingredients": [
        {
            "name": "nonstick cooking spray (Nonstick)",
            "us_amount": "1",
            "metric_amount": "1"
        },
        {
            "name": "medium sweet potatoes (washed and peeled)",
            "us_amount": "4",
            "metric_amount": "4"
        },
        {
            "name": "olive oil",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "ground cinnamon",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "chili powder",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "ground ginger",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        }
    ],
    "instructions": [
        "Preheat oven to 425°F.",
        "Spray baking sheet with nonstick cooking spray.",
        "Cut sweet potatoes into thick strips or wedges and put in a gallon-size Ziploc bag.",
        "Add the olive oil and shake until the sweet potato pieces are coated. Place the uncooked “fries” on the prepared baking sheet in a single layer.",
        "Mix the spices in a small bowl. Sprinkle the spice mix over the sweet potatoes.",
        "Bake for 45 minutes to an hour."
    ],
