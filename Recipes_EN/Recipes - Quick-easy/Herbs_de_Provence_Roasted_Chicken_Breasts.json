    "ingredients": [
        {
            "name": "bone-in, skin on chicken breasts",
            "us_amount": "2 (10-oz) breasts",
            "metric_amount": "2 (10-oz) breasts"
        },
        {
            "name": "olive oil",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        },
        {
            "name": "dried herbs de Provence or crushed rosemary",
            "us_amount": "1 1/2 tsp",
            "metric_amount": "7 g"
        },
        {
            "name": "salt",
            "us_amount": "1/4 tsp",
            "metric_amount": "1 g"
        }
    ],
    "instructions": [
        "Preheat oven to 425°F.",
        "Place the chicken in a baking pan. Brush the chicken skin with the oil. Sprinkle with the herbs de Provence and salt.",
        "Roast until the chicken is well done (internal temperature of at least 165°F) and skin is crisped and browned, about 40–45 minutes.",
        "Let stand for at least 5 minutes. If desired, drizzle the chicken with any pan juices. Serve."
    ],
