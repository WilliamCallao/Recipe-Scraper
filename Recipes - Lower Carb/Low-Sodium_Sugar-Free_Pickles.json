    "ingredients": [
        {
            "name": "white vinegar",
            "us_amount": "1 1/2 cup",
            "metric_amount": "355 ml"
        },
        {
            "name": "black peppercorns",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        },
        {
            "name": "coriander seeds",
            "us_amount": "1 tsp",
            "metric_amount": "5 g"
        },
        {
            "name": "dried dill (or 2 Tbsps. fresh dill)",
            "us_amount": "1 tbsp",
            "metric_amount": "15 ml"
        },
        {
            "name": "stevia",
            "us_amount": "2 tbsp",
            "metric_amount": "30 ml"
        },
        {
            "name": "salt",
            "us_amount": "1/2 tsp",
            "metric_amount": "2 g"
        },
        {
            "name": "crushed red pepper flakes (optional)",
            "us_amount": "1/4 tsp",
            "metric_amount": "1 g"
        },
        {
            "name": "cucumber(s) (sliced to 1/4 inch rounds (about 60 slices))",
            "us_amount": "2",
            "metric_amount": "2"
        },
        {
            "name": "onion(s) (thinly sliced)",
            "us_amount": "1/2",
            "metric_amount": "1/2"
        },
        {
            "name": "garlic (thinly sliced)",
            "us_amount": "2 diente",
            "metric_amount": "2 diente"
        }
    ],
    "instructions": [
        "Combine vinegar, peppercorns, coriander seeds, dill, Stevia, salt, and pepper flakes (optional) in a medium saucepan over high heat. Bring to a boil. Then turn off the heat and set aside.",
        "Combine the cucumbers, onions, and garlic, and tightly pack into a quart-sized glass jar with a lid or a Tupperware container with a lid. Pour the hot liquid over the cucumbers and onions, and ensure all vegetables are completely submerged.",
        "Close the lid tightly and refrigerate for at least 1 day before serving."
    ],
